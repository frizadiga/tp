  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-friza": "^1.1.0",
    "node-sass": "^4.11.0",
    "react-test-renderer": "^16.8.6"
